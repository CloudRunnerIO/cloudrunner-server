#! /bin/bash

# Test for argcomplete:
python -c "import argcomplete" || echo "ArgComplete is not installed. \
Install it using 'pip install argcomplete'"

eval "$(register-python-argcomplete cloudrunner-master)"
eval "$(register-python-argcomplete cloudrunner-dsp)"
eval "$(register-python-argcomplete cloudrunner-trigger)"
